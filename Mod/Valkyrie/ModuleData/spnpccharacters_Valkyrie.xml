                default_group="Infantry"
                level="25"
                civilianTemplate="NPCCharacter.vlandia_troop_civilian_template_t1"
                name="{=GUQ3zfg2}Wolf Knight"
				is_basic_troop="true"
                occupation="Soldier"
                culture="Culture.vlandia">
<!-- This is the ingame face template of the troop all the templates can be found in Bannerlord\Modules\SandBoxCore spcultures.xml-->
